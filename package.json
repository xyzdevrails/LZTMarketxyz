{
  "name": "lztmarket-discord-bot",
  "version": "1.0.0",
  "description": "Bot Discord para vendas de contas Valorant via LZT Market",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test-api": "ts-node src/test-api.ts",
    "test-simple": "ts-node src/test-api-simple.ts",
    "test-endpoints": "ts-node src/test-endpoints.ts",
    "test-valorant": "ts-node src/test-valorant-endpoint.ts",
    "test-user": "ts-node src/test-user-accounts.ts",
    "test-search": "ts-node src/test-market-search.ts",
    "test-list": "ts-node src/test-list-accounts.ts",
